{
	"name": "@devprotocol/clubs-plugin-posts",
	"version": "0.3.0",
	"type": "module",
	"description": "Template repository for using TypeScript",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js"
		}
	},
	"types": "./dist/src/index.d.ts",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "ava || echo 'skip'",
		"preview": "astro --root preview dev",
		"build": "rollup -c",
		"pretest": "yarn build",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,json,md,yml}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"@boringer-avatars/vue3": "^0.2.1",
		"@devprotocol/dev-kit": "8.5.0",
		"@devprotocol/util-ts": "4.0.0",
		"bignumber.js": "9.1.2",
		"dayjs": "^1.11.8",
		"dompurify": "^3.0.5",
		"ethers": "^6.6.4",
		"marked": "9.0.3",
		"photoswipe": "5.4.2",
		"ramda": "0.29.0",
		"redis": "^4.6.7",
		"sass": "1.68.0",
		"svelte": "^4.0.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@astrojs/svelte": "^4.0.2",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^3.0.0",
		"@ava/typescript": "4.1.0",
		"@devprotocol/clubs-core": "1.4.0",
		"@nanostores/vue": "^0.10.0",
		"@redis/json": "^1.0.4",
		"@rollup/plugin-typescript": "11.1.4",
		"@types/dompurify": "^3.0.2",
		"@types/dotenv": "8.2.0",
		"@types/marked": "^5.0.0",
		"@types/node": "18.18.0",
		"@types/ramda": "0.29.5",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"astro": "^3.0.12",
		"ava": "5.3.1",
		"dotenv": "16.3.1",
		"eslint": "8.50.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-functional": "4.4.1",
		"eslint-plugin-tailwind": "^0.2.1",
		"husky": "8.0.3",
		"nanostores": "^0.9.1",
		"prettier": "3.0.3",
		"prettier-plugin-astro": "^0.12.0",
		"prettier-plugin-svelte": "^3.0.0",
		"rimraf": "5.0.5",
		"rollup": "3.29.4",
		"tailwindcss": "3.3.3",
		"txtgen": "^3.0.6",
		"typescript": "5.2.2",
		"vue": "3.3.4"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^1.0.0"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	}
}
