{
	"name": "@devprotocol/clubs-plugin-posts",
	"version": "0.14.0",
	"type": "module",
	"description": "Template repository for using TypeScript",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./plugin-helper": {
			"default": "./dist/plugin-helper/index.js",
			"types": "./dist/plugin-helper/index.d.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js",
		"!**/*.test.*",
		"src",
		"plugin-helper.d.ts"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "vitest",
		"preview": "astro --root preview dev",
		"build": "rollup -c",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test --run && yarn build",
		"prepare": "husky install"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"@boringer-avatars/vue3": "^0.2.1",
		"@devprotocol/dev-kit": "8.6.0",
		"@devprotocol/util-ts": "4.0.0",
		"bignumber.js": "9.1.2",
		"dayjs": "^1.11.8",
		"dompurify": "^3.0.5",
		"ethers": "^6.6.4",
		"marked": "12.0.1",
		"nanoid": "^5.0.6",
		"photoswipe": "5.4.3",
		"ramda": "0.29.1",
		"redis": "^4.6.7",
		"sass": "1.71.1",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@astrojs/svelte": "^5.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@devprotocol/clubs-core": "3.3.1",
		"@nanostores/vue": "^0.10.0",
		"@redis/json": "^1.0.4",
		"@rollup/plugin-typescript": "11.1.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dompurify": "^3.0.2",
		"@types/dotenv": "8.2.0",
		"@types/marked": "^5.0.0",
		"@types/node": "20.11.25",
		"@types/ramda": "0.29.11",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"astro": "4.4.14",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-functional": "4.4.1",
		"eslint-plugin-tailwind": "^0.2.1",
		"husky": "9.0.11",
		"jsdom": "^24.0.0",
		"nanostores": "^0.10.0",
		"prettier": "3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.5",
		"rimraf": "5.0.5",
		"rollup": "4.12.1",
		"rollup-plugin-dts": "^6.0.2",
		"svelte": "^4.2.1",
		"tailwindcss": "3.4.1",
		"txtgen": "^3.0.6",
		"typescript": "5.4.2",
